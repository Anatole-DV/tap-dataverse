    "version": "0.2.0",
    "configurations": [
        {
            "name": "tap-dataverse discovery",
            "type": "debugpy",
            "request": "launch",
            "module": "tap_dataverse.tap",
            "args": [
                "--config",
                ".secrets/config.json"
            ],
            "python": "${command:python.interpreterPath}",
            // Set to true to debug third-party library code
            "justMyCode": false,
        },
        {
            "name": "tap-dataverse env",
            "type": "debugpy",
            "request": "launch",
            "module": "tap_dataverse.tap",
            "args": [
                "--config",
                "ENV"
            ],
            "env": {
                "TAP_DATAVERSE_API_URL": "",
                "TAP_DATAVERSE_RESOURCE": ""
            },
            "python": "${command:python.interpreterPath}",
            // Set to true to debug third-party library code
            "justMyCode": false,
        }
    ]
}